{
  "name": "@choksheak/ts-utils",
  "license": "The Unlicense",
  "version": "0.2.7",
  "description": "Random Typescript utilities with support for full tree-shaking",
  "private": false,
  "scripts": {
    "lint": "eslint src/**",
    "format": "prettier --write \"**/*.ts\"",
    "test": "npm run clean && jest",
    "test:watch": "npm run clean && jest --watch",
    "clean": "rm -rf dist",
    "ci": "npm run lint && npm run format && npm run test",
    "build": "tsup src/*",
    "gen-types": "tsc --emitDeclarationOnly --declaration",
    "docs": "rm -rf docs && typedoc `find src -name '*.ts' ! -name '*.test.ts'` --out docs --name 'Chokâ€™s Typescript Utilities'",
    "pub": "scripts/publish.sh"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.97",
    "eslint": "^9.14.0",
    "fake-indexeddb": "^6.0.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-localstorage-mock": "^2.4.26",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typedoc": "^0.28.3",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.13.0"
  }
}